{
  "name": "Ethereum Gas Price Extension",
  "version": "1.0",
  "description": "Shows the latest current standard gas price provided by EthGasStation",
  "manifest_version": 2,
  "background": {
    "scripts": ["src/eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "static/icon.png",
    "default_title": "Ethereum Gas Price" 
  },
  "permissions": [
    "alarms"
  ]
}